<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_grap"
    app:startDestination="@id/loginFragment">
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.mospertestsapprefactor.presentation.login.LoginFragment"
        android:label="LoginFragment" >
        <action
            android:id="@+id/action_loginFragment_to_employeeListFragment"
            app:destination="@id/employeeListFragment" />
    </fragment>
    <fragment
        android:id="@+id/employeeListFragment"
        android:name="com.example.mospertestsapprefactor.presentation.employee_list.EmployeeListFragment"
        android:label="EmployeeListFragment" >
        <action
            android:id="@+id/action_employeeListFragment_to_detailEmployeeFragment"
            app:destination="@id/detailEmployeeFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailEmployeeFragment"
        android:name="com.example.mospertestsapprefactor.presentation.detail_employee.DetailEmployeeFragment"
        android:label="DetailEmployeeFragment" />
</navigation>